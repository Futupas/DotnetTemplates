{
    "$schema": "http://json.schemastore.org/template",
    "author": "Futupas",
    "classifications": [ "Common", "WebAPI" ],
    "identity": "Futupas.Dotnet6.WebApiTemplate",
    "sourceName": "FutupasWebApi",
    "name": ".NET 6 WebApi project app with Swagger and logging",
    "shortName": "dotnet6webapi",
    "sources": [
        {
            "source":"./",
            "target":"./",
            "include": [
                "**/*"
            ],
            "exclude":[
                "**/.vs/**",
                "**/[Bb]in/**",
                "**/[Oo]bj/**",
                "**/.template.config/**",
                "**/*.filelist",
                "**/*.user",
                "**/*.lock.json"
            ]
        }
    ]
}
